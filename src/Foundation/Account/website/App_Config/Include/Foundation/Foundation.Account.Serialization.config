<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <unicorn>
            <configurations>
                <configuration name="Foundation.Account" description="Foundation.Account" dependencies="Foundation.Serialization" extends="Helix.Base">
                    <predicate>
                        <include name="$(layer).$(module).Rules.Definitions.Tags" database="master" path="/sitecore/system/Settings/Rules/Definitions/Tags/Power Account Status" />
                        <include name="$(layer).$(module).Rules.Definitions.Elements" database="master" path="/sitecore/system/Settings/Rules/Definitions/Elements/Power Account Status" />
                        <include name="$(layer).$(module).Rules.ConditionalRenderings.Tags.Default" database="master" path="/sitecore/system/Settings/Rules/Conditional Renderings/Tags/Default" />
                    </predicate>
                </configuration>
            </configurations>
        </unicorn>
    </sitecore>
</configuration>
